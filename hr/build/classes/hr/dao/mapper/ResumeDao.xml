<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper  namespace="hr.dao.ResumeDao">

<insert id="addResume" parameterType="Resume" >

    insert into resume (name,sex,age,national,school,education,major,native_place,phone,mail,job,last_work,salary_expectation,interest,politics_status)
    values (#{name},#{sex},#{age},#{national},#{school},#{education},#{major},#{nativePlace},#{phone},#{mail},#{job},#{lastWork},#{salaryExpectation},#{interest},#{politicsStatus})
</insert>
 
 	
	
 

</mapper>